{
    "manifest_version": 2,
    "name": "extension_prototype",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/border-48.png"
    },
  
    "content_scripts": [
        {
          "matches": ["*://outlook.office.com/mail/inbox/id/*"],
          "js": ["extension_prototype.js"]
        },
        {
          "matches": ["*://outlook.office365.com/mail/inbox/id/*"],
          "js": ["extension_prototype.js"]
        }
      ]
  }